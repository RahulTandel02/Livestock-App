{
  "expo": {
    "name": "Cattle Farm",
    "slug": "livestock_management",
    "scheme":"myliveapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#C5CE2C"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#C5CE2C"
      },
      "intentFilters":[
        {
          "action":"VIEW",
          "autoVerify":true,
          "data":[
            {
              "scheme":"https",
              "host":"antibilious-bag.000webhostapp.com",
              "pathPrefix":"/openApp"
            }
          ],
          "category":[
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "softwareKeyboardLayoutMode": "pan",
      "package":"com.livestock.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "entryPoint":"./index.js"
  }
}
